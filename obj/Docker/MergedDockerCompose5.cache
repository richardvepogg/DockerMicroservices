  mssqltools:
    command:
      - /bin/bash
      - -c
      - sleep 40 && /opt/mssql-tools/bin/sqlcmd -S sqlserver -U SA -P cadastroprodutos123! -d master -i /tmp/CriarBancoEstoque.sql
    depends_on:
      sqlserver:
        condition: service_started
